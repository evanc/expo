{
  "expo": {
    "name": "sandbox",
    "slug": "sandbox",
    "privacy": "public",
    "sdkVersion": "UNVERSIONED",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-dark.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "android": {
      "splashscreen": {
        "backgroundColor": "#22bb00",
        "imagePath": "./assets/splashscreen-triangle-blue.png",
        "mode": "contain"
      }
    }
  }
}
